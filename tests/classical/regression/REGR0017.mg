% ~~A | (A -> ~~B | (B -> ~~C | (C -> (~~D -> D) | ~D | ~~D)))

%positive A, B, C, D.

%refute ~ ~ A + (A => ~ ~ B + (B => ~ ~ C + (C => (~ ~ D => D) + ~ D + ~ ~ D))).

%negative A, B, C, D.

%refute ~ ~ A + (A => ~ ~ B + (B => ~ ~ C + (C => (~ ~ D => D) + ~ D + ~ ~ D))).
